
@{
    ViewData["Title"] = "Admin users";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<p>
    <a asp-route="@Names.AdminAddUser">Add a new user</a>
</p>

<table>
    <thead>
        <tr>
            <th>Name</th>
            <th>Tools</th>
        </tr>
    </thead>
    <tbody>
        @foreach (User u in Model)
        {
            <tr>
                <td>@u.Name</td>
                <td><a asp-route="@Names.AdminDelUser" asp-route-userId="@u.ID">Delete User</a></td>
            </tr>
        }
    </tbody>
</table>


